<h1>edit</h1>
<div class="booyah-box col-10 offset-1">
  <h1>Edit Your Profile</h1>
  <%= simple_form_for @helpee do |f| %>
    <%= f.input :name %>
    <%= f.input :about %>
    <%= f.input :story %>
    <%= f.input :need %>
    <%= f.input :address %>
    <br />
    <%= f.submit 'Update', class: 'btn btn-primary' %>
  <% end %>
</div>